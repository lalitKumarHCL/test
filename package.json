{
  "name": "@velocity/ucv-ext-ewm",
  "version": "1.1.31",
  "description": "EWM plugin",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf build && babel ./lib --out-dir dist",
    "build-runnable": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --progress",
    "dist": "cross-env BUILD_ENV=production NODE_ENV=production npm run build",
    "dist-runnable": "cross-env BUILD_ENV=production NODE_ENV=production npm run build-runnable",
    "lint": "eslint .",
    "lint:ci": "eslint -f json -o eslint-report.json --ext=js .",
    "start-runnable": "babel-node lib/index-runnable",
    "start": "babel-node lib/start",
    "test": "npm run lint && npm run test-unit:xml && npm run test-func:xml",
    "test:base": "nyc --reporter=cobertura --reporter=lcov --reporter=text mocha --recursive --require @babel/register",
    "test-unit:xml": "npm run test:base -- ./test/unit --reporter mocha-junit-reporter --reporter-options mochaFile=./test-results/unit/results.xml",
    "test-func:xml": "npm run test:base -- ./test/func --timeout 120000 --reporter mocha-junit-reporter --reporter-options mochaFile=./test-results/func/results.xml"
  },
  "files": [
    "dist/**/*.*"
  ],
  "engines": {
    "node": "6.x"
  },
  "license": "IBM Confidential",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@velocity/api-client": "1.0.522",
    "@velocity/logger": "^1.0.100",
